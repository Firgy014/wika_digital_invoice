<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="update_create_po_actions" model="ir.cron">
            <field name="name">Update and Create PO</field>
            <field name="active" eval="False" />
            <field name="model_id" ref="wika_integration.model_wika_get_po" />
            <field name="state">code</field>
            <field name="active">False</field>
            <field name="code">model._auto_update_create_po(1)</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">1</field>
        </record>
        <record id="get_po_actions" model="ir.cron">
            <field name="name">Update PO</field>
            <field name="active" eval="False" />
            <field name="model_id" ref="wika_purchase.model_purchase_order" />
            <field name="state">code</field>
            <field name="active">False</field>
            <field name="code">model._autoupdate_po(1)</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">1</field>
        </record>
        <record id="create_po_actions" model="ir.cron">
            <field name="name">Get PO</field>
            <field name="active" eval="False" />
            <field name="model_id" ref="wika_integration.model_wika_get_po" />
            <field name="state">code</field>
            <field name="active">True</field>
            <field name="code">model._autocreate_po()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">1</field>
        </record>
        <record id="get_payment_status" model="ir.cron">
            <field name="name">Create Payment from SAP payment status</field>
            <field name="active" eval="False" />
            <field name="model_id" ref="wika_integration.model_wika_get_payment_status" />
            <field name="state">code</field>
            <field name="active">False</field>
            <field name="code">model._auto_get_payment_status('2024/04/30', '2024/04/30', '5100000018')</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">1</field>
        </record>
        <record id="get_create_update_invoice_non_po" model="ir.cron">
            <field name="name">Create Invoice non PO from SAP</field>
            <field name="active" eval="False" />
            <field name="model_id" ref="wika_integration.model_wika_get_invoice_non_po" />
            <field name="state">code</field>
            <field name="active">False</field>
            <field name="code">model.get_create_update_invoice_non_po('2024-03-01', '2024-03-30', '')</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">1</field>
        </record>
    </data>
</odoo>