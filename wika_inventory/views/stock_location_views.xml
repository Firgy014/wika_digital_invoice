<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_location_form_branch" model="ir.ui.view">
        <field name="name">stock.location.form.branch</field>
        <field name="model">stock.location</field>
        <field name="inherit_id" ref="stock.view_location_form"/>
        <field name="arch" type="xml">
            <field name="usage" position="after">
                <field name="project_id" />
                <field name="divisi_id" />
            </field>
        </field>
    </record>

    <record id="view_location_tree2_branch" model="ir.ui.view">
        <field name="name">stock.location.tree.branch</field>
        <field name="model">stock.location</field>
        <field name="inherit_id" ref="stock.view_location_tree2"/>
        <field name="arch" type="xml">
            <field name="usage" position="after">
                <field name="project_id" />
                <field name="divisi_id" />
            </field>
        </field>
    </record>
   
</odoo>
