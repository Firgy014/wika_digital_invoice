<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="review_action" model="ir.actions.act_window">
            <field name="name">Review</field>
            <field name="res_model">wika.mcs.budget.review</field>
            <field name="context">{}</field>
            <field name="domain">[]</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('wika_budget_review.review_tree_view')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('wika_budget_review.review_form_view')})]"/>
        </record>

        <record id="budget_bad_action" model="ir.actions.act_window">
            <field name="name">BAD</field>
            <field name="res_model">wika.mcs.budget.coa</field>
            <field name="context">{'default_tipe_budget': 'bad'}</field>
            <field name="domain">[('tipe_budget','=','bad')]</field>
            <!-- <field name="view_type">form</field> -->
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('budget_bad_tree_view')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('budget_bad_form_view')})
                ]"/>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Master Data BAD
                </p>
                <p>
                    Silahkan Buat Data Baru!
                </p>
            </field>
        </record>


        
    </data>
</odoo>
