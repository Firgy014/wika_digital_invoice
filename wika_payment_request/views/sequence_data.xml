<odoo>
    <data> 
        <!-- Sequence -->
 
        <record id="seq_wika_req_payment" model="ir.sequence">
            <field name="name">Payment Request</field>
            <field name="code">wika.payment.request</field>
            <field name="prefix">PR/%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_wika_partial_req_payment" model="ir.sequence">
            <field name="name">Partial Payment Request</field>
            <field name="code">wika.partial.payment.request</field>
            <field name="prefix">Partial/%(year)s/%(month)s/</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>