<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="view_wika_account_move_form_inherit" model="ir.ui.view">
            <field name="name">wika.account.move.inherit.view.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_id']" position="after">
                    <field name="bap_id"/>
                </xpath>
                <xpath expr="//field[@name='bap_id']" position="after">
                    <field name="branch_id"/>
                </xpath>
                <xpath expr="//field[@name='branch_id']" position="after">
                    <field name="department_id"/>
                </xpath>
                <xpath expr="//field[@name='department_id']" position="after">
                    <field name="project_id"/>
                </xpath>
                <xpath expr="//field[@name='project_id']" position="after">
                    <field name="pr_id"/>
                </xpath>
                
                <xpath expr="//notebook/page[@name='invoice_line_ids']" position="after">
                    <page string="List Document">
                        <field name="document_ids">
                            <tree editable="top">
                                <field name="document_id"/>
                                <field name="document"/>
                                <field name="filename" invisible="1"/>
                                <field name="state"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>
    
    </data>
    

</odoo>
