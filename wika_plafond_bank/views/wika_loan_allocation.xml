<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="wika_loan_allocation_view_tree" model="ir.ui.view">
            <field name="name">wika.loan.allocation.view.tree</field>
            <field name="model">wika.loan.allocation</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="code"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="wika_loan_allocation_view_form" model="ir.ui.view">
            <field name="name">wika.loan.allocation.view.form</field>
            <field name="model">wika.loan.allocation</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <field name="code"/>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="wika_loan_allocation_action" model="ir.actions.act_window">
            <field name="name">Journal Entries</field>
            <field name="res_model">wika.loan.allocation</field>
            <field name="view_mode">tree,form</field>
        </record>
    
    </data>
    
</odoo>


